// These are prebuilt java and android libraries needed for
// WorkManager, Room and RxJava. AOSP prebuilt sdk has only an outdated version of room
// compiler and it's dependencies which does not support Flowables from RxJava3

android_library_import {
    name: "androidx.lifecycle_lifecycle-reactivestreams-nodeps",
    aars: ["androidx/lifecycle/lifecycle-reactivestreams-2.3.0-alpha05.aar"],
    sdk_version: "29",
}

android_library {
    name: "androidx.lifecycle_lifecycle-reactivestreams",
    static_libs: [
        "androidx.lifecycle_lifecycle-reactivestreams-nodeps",
    ],
    min_sdk_version: "14",
    sdk_version: "29",
    manifest: "androidx/lifecycle/AndroidManifest.xml",
}

android_library_import {
    name: "androidx.room_room-runtime-stable-nodeps",
    aars: ["androidx/room-runtime/room-runtime-2.3.0.aar"],
    sdk_version: "current",
}

android_library {
    name: "androidx.room_room-runtime-stable",
    static_libs: [
        "androidx.room_room-runtime-stable-nodeps",
        "androidx.room_room-common-stable",
        "androidx.sqlite_sqlite-framework",
        "androidx.sqlite_sqlite",
        "androidx.arch.core_core-runtime",
    ],
    min_sdk_version: "14",
    sdk_version: "current",
    manifest: "androidx/room-runtime/AndroidManifest.xml",
}

android_library_import {
    name: "androidx.room_room-rxjava3-nodeps",
    aars: ["androidx/room-rxjava/room-rxjava3-2.3.0.aar"],
    sdk_version: "current",
}

android_library {
    name: "androidx.room_room-rxjava3",
    static_libs: [
        "androidx.room_room-rxjava3-nodeps",
    ],
    min_sdk_version: "14",
    sdk_version: "current",
    manifest: "androidx/room-rxjava/AndroidManifest.xml",
}

android_library_import {
    name: "androidx.work_work-runtime-nodeps",
    aars: ["androidx/work-runtime/work-runtime-2.5.0.aar"],
    sdk_version: "current",
}

android_library {
    name: "androidx.work_work-runtime",
    static_libs: [
        "androidx.work_work-runtime-nodeps",
        "androidx.paging_paging-common",
        "guava",
    ],
    min_sdk_version: "14",
    sdk_version: "current",
    manifest: "androidx/work-runtime/AndroidManifest.xml",
}

android_library_import {
    name: "rxandroid-nodeps",
    aars: ["rxandroid/rxandroid-3.0.0.aar"],
    sdk_version: "current",
}

android_library {
    name: "rxandroid",
    static_libs: [
        "rxandroid-nodeps",
    ],
    min_sdk_version: "9",
    sdk_version: "current",
    manifest: "rxandroid/AndroidManifest.xml",
}

java_import {
    name: "rxjava3",
    jars: ["rx/rxjava-3.0.13.jar"],
    sdk_version: "current",
}

java_import {
    name: "reactivestreams",
    jars: ["rx/reactive-streams-1.0.3.jar"],
    sdk_version: "current",
}

java_import {
    name: "kotlin-stdlib-latest",
    host_supported: true,
    jars: ["kotlin/kotlin-stdlib-1.4.31.jar"],
    sdk_version: "current",
}

java_import {
    name: "kotlinx-metadata-jvm-latest",
    jars: ["kotlin/kotlinx-metadata-jvm-0.2.0.jar"],
    host_supported: true,
}

java_import {
    name: "androidx.room_room-common-stable-nodeps",
    jars: ["androidx/room/room-common-2.3.0.jar"],
    sdk_version: "current",
    host_supported: true,
}

java_library {
    name: "androidx.room_room-common-stable",
