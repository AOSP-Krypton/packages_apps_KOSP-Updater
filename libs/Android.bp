    sdk_version: "current",
    host_supported: true,
}

java_library {
    name: "androidx.room_room-migration-stable",
    static_libs: [
        "androidx.room_room-migration-stable-nodeps",
        "androidx.room_room-common-stable",
        "kotlin-stdlib-latest",
        "gson-prebuilt-jar",
    ],
    sdk_version: "current",
    host_supported: true,
}

java_import {
    name: "androidx.room_room-compiler-processing-stable-nodeps",
    jars: ["androidx/room/room-compiler-processing-2.3.0.jar"],
    sdk_version: "current",
    host_supported: true,
}

java_library {
    name: "androidx.room_room-compiler-processing-stable",
    static_libs: [
        "androidx.annotation_annotation",
        "androidx.room_room-compiler-processing-stable-nodeps",
        "auto-common",
        "auto-value-annotations",
        "guava",
        "kotlin-stdlib-latest",
        "kotlinx-metadata-jvm-latest",
    ],
    sdk_version: "current",
