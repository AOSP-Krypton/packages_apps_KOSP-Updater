}

java_import_host {
    name: "androidx.room_room-compiler-stable-nodeps",
    jars: ["androidx/room/room-compiler-2.3.0.jar"],
    sdk_version: "current",
}

java_library_host {
    name: "androidx.room_room-compiler-stable",
    static_libs: [
        "androidx.room_room-common-stable",
        "androidx.room_room-compiler-processing-stable",
        "androidx.room_room-compiler-stable-nodeps",
        "androidx.room_room-migration-stable",
        "kotlin-stdlib-latest",
        "auto-common",
        "auto-value-annotations",
        "javapoet-prebuilt-jar",
        "xerial-sqlite-jdbc",
        "kotlinx-metadata-jvm-latest",
        "commons-codec",
        "jetbrains-annotations",
        "guava",
    ],
}

java_plugin {
    name: "androidx.room_room-compiler-plugin-stable",
    static_libs: [
        "androidx.room_room-compiler-stable",
        "kotlin-reflect"
    ],
    processor_class: "androidx.room.RoomProcessor",
    generates_api: true,
}
